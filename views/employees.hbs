                                    {{/if}}
                                </div>
                                <div class="row">
                                    <table class="table table-dark table-striped table-bordered">
                                        <thead class="text-center">
                                            <tr>
                                                <th scope="col">Name</th>
                                                <th scope="col">Email</th>
                                                <th scope="col">Shift</th>
                                                <th scope="col" colspan="2">Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each users}}
                                            <tr>
                                                <td class="text-center">
                                                    {{first_name}}<span> </span>{{last_name}}
                                                </td>
                                                <td class="text-center px-3">
                                                    {{email}}
                                                </td>
                                                <td class="text-center px-3">
                                                    {{time_start}}<span> - </span>{{time_end}}
                                                </td>
                                                <td class="px-3 text-center">
                                                    <button class="btn btn-warning" data-bs-toggle="modal" data-bs-target="#myModal{{employee_id}}">Edit</button>
                                                    <div class="modal fade" id="myModal{{employee_id}}" tabindex="-1" role="dialog">
                                                        <div class="modal-dialog" role="document">
                                                            <form action="/admin/edit" method="POST" class="form">
                                                                <div class="modal-content">
                                                                    <div class="modal-header">
                                                                        <h5 class="modal-title">Edit Employee</h5>
                                                                    </div>
                                                                    <div class="modal-body">
                                                                        <input type="hidden" name="employee_id" id="employee_id" value="{{employee_id}}">
                                                                        <input type="text" name="first_name" id="first_name" placeholder="Enter First name" class="form-control mb-3" value="{{first_name}}" required>
                                                                        <input type="text" name="last_name" id="last_name" placeholder="Enter Stock" class="form-control mb-3" value="{{last_name}}" required>
                                                                        <input type="email" name="email" id="email" placeholder="Enter Email" class="form-control mb-3" value="{{email}}" required>
                                                                        <select name="shift_id" id="shift_id" class="form-select mb-3">
                                                                            <option value="1">08:00 AM - 05:00 AM</option>
                                                                            <option value="2">06:00 PM - 03:00 AM</option>
                                                                            <option value="3">12:00 PM - 09:00 PM</option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="modal-footer">
                                                                        <button class="btn btn-success" type="submit" name="edit" id="edit" >Save Changes</button>
                                                                        <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
                                                                    </div>
                                                                </div>
                                                            </form>
                                                        </div>
                                                    </div>
                                                </td>
                                                <td class="px-3 text-center">
                                                    <a class="btn btn-danger" href="/admin/delete/{{employee_id}}">Delete</a>
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div> 
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">                            
                    <div class="modal fade" id="AddModal" tabindex="-1" role="dialog">
                        <div class="modal-dialog" role="document">
                            <form action="/admin/add" method="POST" class="form">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title">Add Employee</h5>
                                    </div>
                                    <div class="modal-body">
                                        <input type="text" name="first_name" id="first_name" placeholder="Enter First name" class="form-control mb-3"  required>
                                        <input type="text" name="last_name" id="last_name" placeholder="Enter Last Name" class="form-control mb-3"  required>
                                        <input type="email" name="email" id="email" placeholder="Enter Email" class="form-control mb-3"  required>
                                        <input type="password" name="password" id="password" placeholder="Enter Password" class="form-control mb-3"  required>
                                        <input type="password" name="cpassword" id="cpassword" placeholder="Confirm Password" class="form-control mb-3"  required>
                                        <select name="shift_id" id="shift_id" class="form-select mb-3">
                                            <option value="">Select Shift</option>
                                            <option value="1">08:00 AM - 05:00 AM</option>
                                            <option value="2">06:00 PM - 03:00 AM</option>
                                            <option value="3">12:00 PM - 09:00 PM</option>
                                        </select>
                                    </div>
                                    <div class="modal-footer">
                                        <button class="btn btn-success" type="submit" name="add" id="add" >Add</button>
                                        <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
                                    </div>
                                </div>
